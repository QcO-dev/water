/* Avoids matching string addresses */
function h() = "He" + "llo";

function main() {
	println(2 == 2);
	println("Hello" == h());
	println("hi" != h());
	println("Hello" === h());
	println("hi" !== h());
	println(5 != 5);

	println(h() === null);
	println(h() !== null);

	println(2 < 3);
	println(3 > 2);
	println(5 <= 5);
	println(4 <= 2);
}